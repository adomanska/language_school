<Page x:Class="LanguageSchool.Presentation.StudentsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:controls="clr-namespace:LanguageSchool.Presentation" 
      xmlns:local="clr-namespace:LanguageSchool.Presentation"
      mc:Ignorable="d" 
      d:DesignHeight="600" d:DesignWidth="1000"
      Title="StudentsPage">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="435"/>
            <ColumnDefinition Width="Auto"/>
        </Grid.ColumnDefinitions>
        <Button Grid.Column="1" Name="goToStartPage" HorizontalAlignment="Right" Margin="300,215,0,0"
                Width="100" Height="50"
                Click="goToStartPage_Click">
            Go To Start Page
        </Button>
        <controls:StudentRegistrationView x:Name="studentRegistrationControl" Grid.Column="0" Width="435" HorizontalAlignment="Left" Margin="-9,55,0,202"/>
    </Grid>
</Page>
